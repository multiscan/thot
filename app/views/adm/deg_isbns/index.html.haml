-content_for(:title) do
  Degenerate ISBN numbers
  %small and book counts

%table.table
  %thead
    %th ISBN
    %th Books
    %th
  %tbody
    - @degisbns.each do |di|
      %tr
        %td=di.isbn
        %td=di.count
        %td
          =link_to "merger", edit_adm_deg_isbn_path(di)
          \|
          =link_to "details", [:adm, di]
= will_paginate @degisbns
